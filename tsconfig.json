{
  "compilerOptions": {
    "types": ["gas-types-detailed", "jest", "node"],
    "lib": ["ESNext", "dom"],
    "jsx": "react-jsx",
    "baseUrl": "src",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strict": true,
    "paths": {
      "src/*": ["src/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
