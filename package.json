{
  "name": "data-helper-sheets",
  "scripts": {
    "test": "jest",
    "test:integration": "jest --forceExit test/local-development.test",
    "test:integration:extended": "cross-env IS_EXTENDED=1 jest --forceExit test/local-development.test",
    "login": "clasp login",
    "setup": "rimraf .clasp.json && clasp create --type sheets --title \"Timemap Sheets Addon\" --rootDir ./dist",
    "open": "clasp open --addon",
    "setup:https": "mkdirp certs && mkcert -key-file ./certs/key.pem -cert-file ./certs/cert.pem localhost 127.0.0.1",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build": "cross-env NODE_ENV=production webpack",
    "deploy:dev": "cp .dev.clasp.json .clasp.json && rimraf dist && npm run build:dev && clasp push",
    "deploy:prod": "cp .prod.clasp.json .clasp.json &&  rimraf dist && npm run build && clasp push",
    "serve": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm run deploy:dev && npm run serve",
    "types": "tsc --noEmit --skipLibCheck",
    "clasp": "clasp"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
